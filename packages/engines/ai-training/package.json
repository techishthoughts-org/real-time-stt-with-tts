{
  "name": "@voice/ai-training",
  "version": "0.1.0",
  "description": "Advanced AI training and model management for Gon Voice Assistant",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx watch src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "typecheck": "tsc -p tsconfig.json --noEmit"
