    "tensorflow": "^4.17.0",
    "natural": "^6.12.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "tsx": "^4.16.0",
    "typescript": "^5.5.0",
    "vitest": "^2.1.9"
  },
  "peerDependencies": {
    "node": ">=18.0.0"
  }
}
